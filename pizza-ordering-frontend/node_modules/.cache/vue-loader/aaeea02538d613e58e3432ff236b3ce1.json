ass=\"{'p-6':!hasTable}\">\n      <slot />\n    </div>\n  </component>\n</template>\n\n<script>\nimport Icon from '@/components/Icon'\n\nexport default {\n  name: 'CardComponent',\n  components: { Icon },\n  props: {\n    title: String,\n    icon: String,\n    headerIcon: String,\n    hasTable: Boolean,\n    empty: Boolean,\n    form: Boolean,\n    mb: {\n      type: String,\n      default: 'mb-6 last:mb-0'\n    }\n  },\n  emits: ['header-icon-click', 'submit'],\n  setup (props, { emit }) {\n    const headerIconClick = () => {\n      emit('header-icon-click')\n    }\n\n    const submit = e => {\n      emit('submit', e)\n    }\n\n    return { headerIconClick, submit }\n  }\n}\n</script>\n"]}]}