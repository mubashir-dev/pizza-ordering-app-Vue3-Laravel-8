{"ast":null,"code":"import { ref, computed } from 'vue';\nimport { useStore } from 'vuex';\nimport { mdiMonitorCellphone, mdiSelectColor, mdiFeather, mdiInformationOutline, mdiCheckCircleOutline, mdiAlertCircle, mdiAlertCircleOutline, mdiOpenInNew, mdiReload, mdiPlusCircle, mdiBroadcast, mdiLifebuoy, mdiClose } from '@mdi/js';\nimport MainSection from '@/components/MainSection';\nimport HeroBar from '@/components/HeroBar';\nimport TitleBar from '@/components/TitleBar';\nimport CardComponent from '@/components/CardComponent';\nimport JbButtons from '@/components/JbButtons';\nimport JbButton from '@/components/JbButton';\nimport Notification from '@/components/Notification';\nimport Divider from '@/components/Divider';\nimport ModalBox from '@/components/ModalBox';\nimport TitledSection from '@/components/TitledSection';\nimport Field from '@/components/Field';\nimport CheckRadioPicker from '@/components/CheckRadioPicker';\nimport BottomOtherPagesSection from '@/components/BottomOtherPagesSection';\nexport default {\n  name: 'Ui',\n  components: {\n    MainSection: MainSection,\n    HeroBar: HeroBar,\n    TitleBar: TitleBar,\n    CardComponent: CardComponent,\n    JbButtons: JbButtons,\n    JbButton: JbButton,\n    Notification: Notification,\n    Divider: Divider,\n    ModalBox: ModalBox,\n    TitledSection: TitledSection,\n    CheckRadioPicker: CheckRadioPicker,\n    Field: Field,\n    BottomOtherPagesSection: BottomOtherPagesSection\n  },\n  setup: function setup() {\n    var titleStack = ref(['Admin', 'UI Components']);\n    var modalOneActive = ref(false);\n    var modalTwoActive = ref(false);\n    var modalThreeActive = ref(false);\n    var notificationSettingsModel = ref([]);\n    var notificationsOutline = computed(function () {\n      return notificationSettingsModel.value.indexOf('outline') > -1;\n    });\n    var buttonSettingsModel = ref([]);\n    var buttonsOutline = computed(function () {\n      return buttonSettingsModel.value.indexOf('outline') > -1;\n    });\n    var buttonsSmall = computed(function () {\n      return buttonSettingsModel.value.indexOf('small') > -1;\n    });\n    var store = useStore();\n\n    var toast = function toast() {\n      store.dispatch('pushMessage', 'Welcome back. This is demo');\n    };\n\n    return {\n      titleStack: titleStack,\n      modalOneActive: modalOneActive,\n      modalTwoActive: modalTwoActive,\n      modalThreeActive: modalThreeActive,\n      notificationSettingsModel: notificationSettingsModel,\n      notificationsOutline: notificationsOutline,\n      buttonSettingsModel: buttonSettingsModel,\n      buttonsOutline: buttonsOutline,\n      buttonsSmall: buttonsSmall,\n      toast: toast,\n      mdiMonitorCellphone: mdiMonitorCellphone,\n      mdiSelectColor: mdiSelectColor,\n      mdiFeather: mdiFeather,\n      mdiInformationOutline: mdiInformationOutline,\n      mdiCheckCircleOutline: mdiCheckCircleOutline,\n      mdiAlertCircle: mdiAlertCircle,\n      mdiAlertCircleOutline: mdiAlertCircleOutline,\n      mdiOpenInNew: mdiOpenInNew,\n      mdiReload: mdiReload,\n      mdiPlusCircle: mdiPlusCircle,\n      mdiBroadcast: mdiBroadcast,\n      mdiLifebuoy: mdiLifebuoy,\n      mdiClose: mdiClose\n    };\n  }\n};","map":{"version":3,"sources":["C:\\Users\\dev\\Desktop\\vue-basic\\pizza-ordering-app\\src\\views\\Ui.vue"],"names":[],"mappings":"AA4NA,SAAS,GAAT,EAAc,QAAd,QAA8B,KAA9B;AACA,SAAS,QAAT,QAAyB,MAAzB;AACA,SACE,mBADF,EAEE,cAFF,EAGE,UAHF,EAIE,qBAJF,EAKE,qBALF,EAME,cANF,EAOE,qBAPF,EAQE,YARF,EASE,SATF,EAUE,aAVF,EAWE,YAXF,EAYE,WAZF,EAaE,QAbF,QAcO,SAdP;AAeA,OAAO,WAAP,MAAwB,0BAAxB;AACA,OAAO,OAAP,MAAoB,sBAApB;AACA,OAAO,QAAP,MAAqB,uBAArB;AACA,OAAO,aAAP,MAA0B,4BAA1B;AACA,OAAO,SAAP,MAAsB,wBAAtB;AACA,OAAO,QAAP,MAAqB,uBAArB;AACA,OAAO,YAAP,MAAyB,2BAAzB;AACA,OAAO,OAAP,MAAoB,sBAApB;AACA,OAAO,QAAP,MAAqB,uBAArB;AACA,OAAO,aAAP,MAA0B,4BAA1B;AACA,OAAO,KAAP,MAAkB,oBAAlB;AACA,OAAO,gBAAP,MAA6B,+BAA7B;AACA,OAAO,uBAAP,MAAoC,sCAApC;AAEA,eAAe;AACb,EAAA,IAAI,EAAE,IADO;AAEb,EAAA,UAAU,EAAE;AACV,IAAA,