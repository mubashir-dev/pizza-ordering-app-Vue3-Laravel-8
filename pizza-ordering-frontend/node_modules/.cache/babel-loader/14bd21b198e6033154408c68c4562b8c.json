{"ast":null,"code":"import { computed, ref } from \"vue\";\nimport { useStore } from \"vuex\";\nimport { mdiForwardburger, mdiBackburger, mdiClose, mdiDotsVertical, mdiMenu, mdiClockOutline, mdiCloud, mdiCrop, mdiAccount, mdiCogOutline, mdiEmail, mdiLogout, mdiGithub } from \"@mdi/js\";\nimport NavBarItem from \"@/components/NavBarItem\";\nimport NavBarItemLabel from \"@/components/NavBarItemLabel\";\nimport NavBarMenu from \"@/components/NavBarMenu\";\nimport NavBarMenuDivider from \"@/components/NavBarMenuDivider\";\nimport UserAvatar from \"@/components/UserAvatar\";\nimport Icon from \"@/components/Icon\";\nexport default {\n  name: \"NavBar\",\n  components: {\n    UserAvatar: UserAvatar,\n    NavBarMenu: NavBarMenu,\n    NavBarItem: NavBarItem,\n    NavBarItemLabel: NavBarItemLabel,\n    NavBarMenuDivider: NavBarMenuDivider,\n    Icon: Icon\n  },\n  setup: function setup() {\n    var store = useStore();\n    var isNavBarVisible = computed(function () {\n      return !store.state.isFormScreen;\n    });\n    var isAsideMobileExpanded = computed(function () {\n      return store.state.isAsideMobileExpanded;\n    });\n    var userName = computed(function () {\n      return store.state.userName;\n    });\n    var menuToggleMobileIcon = computed(function () {\n      return isAsideMobileExpanded.value ? mdiBackburger : mdiForwardburger;\n    });\n\n    var menuToggleMobile = function menuToggleMobile() {\n      return store.dispatch(\"asideMobileToggle\");\n    };\n\n    var isMenuNavBarActive = ref(false);\n    var menuNavBarToggleIcon = computed(function () {\n      return isMenuNavBarActive.value ? mdiClose : mdiDotsVertical;\n    });\n\n    var menuNavBarToggle = function menuNavBarToggle() {\n      isMenuNavBarActive.value = !isMenuNavBarActive.value;\n    };\n\n    var menuOpenLg = function menuOpenLg() {\n      store.dispatch(\"asideLgToggle\", true);\n    };\n\n    return {\n      isNavBarVisible: isNavBarVisible,\n      isAsideMobileExpanded: isAsideMobileExpanded,\n      userName: userName,\n      menuToggleMobileIcon: menuToggleMobileIcon,\n      menuToggleMobile: menuToggleMobile,\n      isMenuNavBarActive: isMenuNavBarActive,\n      menuNavBarToggleIcon: menuNavBarToggleIcon,\n      menuNavBarToggle: menuNavBarToggle,\n      menuOpenLg: menuOpenLg,\n      mdiMenu: mdiMenu,\n      mdiClockOutline: mdiClockOutline,\n      mdiCloud: mdiCloud,\n      mdiCrop: mdiCrop,\n      mdiAccount: mdiAccount,\n      mdiCogOutline: mdiCogOutline,\n      mdiEmail: mdiEmail,\n      mdiLogout: mdiLogout,\n      mdiGithub: mdiGithub\n    };\n  }\n};","map":{"version":3,"sources":["C:\\Users\\dev\\Desktop\\vue-basic\\pizza-ordering-app\\src\\components\\NavBar.vue"],"names":[],"mappings":"AAwGA,SAAS,QAAT,EAAmB,GAAnB,QAA8B,KAA9B;AACA,SAAS,QAAT,QAAyB,MAAzB;AACA,SACE,gBADF,EAEE,aAFF,EAGE,QAHF,EAIE,eAJF,EAKE,OALF,EAME,eANF,EAOE,QAPF,EAQE,OARF,EASE,UATF,EAUE,aAVF,EAWE,QAXF,EAYE,SAZF,EAaE,SAbF,QAcO,SAdP;AAeA,OAAO,UAAP,MAAuB,yBAAvB;AACA,OAAO,eAAP,MAA4B,8BAA5B;AACA,OAAO,UAAP,MAAuB,yBAAvB;AACA,OAAO,iBAAP,MAA8B,gCAA9B;AACA,OAAO,UAAP,MAAuB,yBAAvB;AACA,OAAO,IAAP,MAAiB,mBAAjB;AAEA,eAAe;AACb,EAAA,IAAI,EAAE,QADO;AAEb,EAAA,UAAU,EAAE;AACV,IAAA,UAAU,EAAV,UADU;AAEV,IAAA,UAAU,EAAV,UAFU;AAGV,IAAA,UAAU,EAAV,UAHU;AAIV,IAAA,eAAe,EAAf,eAJU;AAKV,IAAA,iBAAiB,EAAjB,iBALU;AAMV,IAAA,IAAI,EAAJ;AANU,GAFC;AAUb,EAAA,KAVa,mBAUL;AACN,QAAM,KAAI,GAAI,QAAQ,EAAtB;AAEA,QAAM,eAAc,GAAI,QAAQ,CAAC;AAAA,aAAM,CAAC,KAAK,CAAC,KAAN,CAAY,YAAnB;AAAA,KAAD,CAAhC;AAEA,QAAM,qBAAoB,GAAI,QAAQ,CACpC;AAAA,aAAM,KAAK,CAAC,KAAN,CAAY,qBAAlB;AAAA,KADoC,CAAtC;AAIA,QAAM,QAAO,GAAI,QAAQ,CAAC;AAAA,aAAM,KAAK,CAAC,KAAN,CAAY,QAAlB;AAAA,KAAD,CAAzB;AAEA,QAAM,oBAAmB,GAAI,QAAQ,CAAC;AAAA,aACpC,qBAAqB,CAAC,KAAtB,GAA8B,aAA9B,GAA8C,gBADV;AAAA,KAAD,CAArC;;AAIA,QAAM,gBAAe,GAAI,SAAnB,gBAAmB;AAAA,aAAM,KAAK,CAAC,QAAN,CAAe,mBAAf,CAAN;AAAA,KAAzB;;AAEA,QAAM,kBAAiB,GAAI,GAAG,CAAC,KAAD,CAA9B;AAEA,QAAM,oBAAmB,GAAI,QAAQ,CAAC;AAAA,aACpC,kBAAkB,CAAC,KAAnB,GAA2B,QAA3B,GAAsC,eADF;AAAA,KAAD,CAArC;;AAIA,QAAM,gBAAe,GAAI,SAAnB,gBAAmB,