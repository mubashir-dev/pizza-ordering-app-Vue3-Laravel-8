rtData = ref(null)\n\n    const fillChartData = () => {\n      chartData.value = chartConfig.sampleChartData()\n    }\n\n    onMounted(() => {\n      fillChartData()\n    })\n\n    return {\n      titleStack,\n      chartData,\n      fillChartData,\n      mdiAccountMultiple,\n      mdiCartOutline,\n      mdiChartTimelineVariant,\n      mdiFinance,\n      mdiMonitorCellphone,\n      mdiReload,\n      mdiGithub\n    }\n  }\n}\n</script>\n"],"sourceRoot":""},"metadata":{},"sourceType":"module"}