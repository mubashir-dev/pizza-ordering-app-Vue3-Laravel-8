{"ast":null,"code":"import { ref } from 'vue';\nimport { mdiMonitorCellphone, mdiAccountMultiple, mdiTableBorder, mdiTableOff } from '@mdi/js';\nimport MainSection from '@/components/MainSection';\nimport Notification from '@/components/Notification';\nimport ClientsTable from '@/components/ClientsTable';\nimport CardComponent from '@/components/CardComponent';\nimport TitleBar from '@/components/TitleBar';\nimport HeroBar from '@/components/HeroBar';\nimport BottomOtherPagesSection from '@/components/BottomOtherPagesSection';\nexport default {\n  name: 'Tables',\n  components: {\n    MainSection: MainSection,\n    HeroBar: HeroBar,\n    TitleBar: TitleBar,\n    CardComponent: CardComponent,\n    ClientsTable: ClientsTable,\n    Notification: Notification,\n    BottomOtherPagesSection: BottomOtherPagesSection\n  },\n  setup: function setup() {\n    var titleStack = ref(['Admin', 'Tables']);\n    return {\n      titleStack: titleStack,\n      mdiMonitorCellphone: mdiMonitorCellphone,\n      mdiAccountMultiple: mdiAccountMultiple,\n      mdiTableBorder: mdiTableBorder,\n      mdiTableOff: mdiTableOff\n    };\n  }\n};","map":{"version":3,"sources":["C:\\Users\\dev\\Desktop\\vue-basic\\pizza-ordering-app\\src\\views\\Tables.vue"],"names":[],"mappings":"AA+BA,SAAS,GAAT,QAAoB,KAApB;AACA,SAAS,mBAAT,EAA8B,kBAA9B,EAAkD,cAAlD,EAAkE,WAAlE,QAAqF,SAArF;AACA,OAAO,WAAP,MAAwB,0BAAxB;AACA,OAAO,YAAP,MAAyB,2BAAzB;AACA,OAAO,YAAP,MAAyB,2BAAzB;AACA,OAAO,aAAP,MAA0B,4BAA1B;AACA,OAAO,QAAP,MAAqB,uBAArB;AACA,OAAO,OAAP,MAAoB,sBAApB;AACA,OAAO,uBAAP,MAAoC,sCAApC;AAEA,eAAe;AACb,EAAA,IAAI,EAAE,QADO;AAEb,EAAA,UAAU,EAAE;AACV,IAAA,WAAW,EAAX,WADU;AAEV,IAAA,OAAO,EAAP,OAFU;AAGV,IAAA,QAAQ,EAAR,QAHU;AAIV,IAAA,aAAa,EAAb,aAJU;AAKV,IAAA,YAAY,EAAZ,YALU;AAMV,IAAA,YAAY,EAAZ,YANU;AAOV,IAAA,uBAAsB,EAAtB;AAPU,GAFC;AAWb,EAAA,KAXa,mBAWJ;AACP,QAAM,UAAS,GAAI,GAAG,CAAC,CAAC,OAAD,EAAU,QAAV,CAAD,CAAtB;AAEA,WAAO;AACL,MAAA,UAAU,EAAV,UADK;AAEL,MAAA,mBAAmB,EAAnB,mBAFK;AAGL,MAAA,kBAAkB,EAAlB,kBAHK;AAIL,MAAA,cAAc,EAAd,cAJK;AAK